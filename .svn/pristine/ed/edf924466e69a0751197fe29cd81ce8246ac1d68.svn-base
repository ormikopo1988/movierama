}

#selectBox {
	padding: 0.2em 0.6em 0.3em;
}

#basic-addon {
	padding-bottom: 5px;
}

.table-responsive
{
    overflow-x: auto;
}

#addAddress {
	float: right !important;
}

#addEducation {
  float: right !important;
}

.fa:not(.smallFa) {
	font: x-large/1 FontAwesome !important;
}

.endorse-item-name {
    border-radius: 0 3px 3px 0;
}

.endorse-item-name {
    float: left;
    line-height: 24px;
}

.endorse-item-name {
    background-color: GrayText;
    padding-right: 10px;
    border-bottom: 1px solid #ddd;
    border-radius: 3px;
    border-right: 1px solid #ddd;
    color: white;
    font-size: 13px;
    font-weight: normal;
    line-height: 20px;
    padding: 0 20px;
    position: relative;
    margin: 15px 10px;
}

.endorse-item-name-text {
	font-weight: bold;
}

.circle {
  display: block;
  width: 200px;
  height: 200px;
  margin: 1em auto;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center center;
  -webkit-border-radius: 99em;
  -moz-border-radius: 99em;
  border-radius: 99em;
  border: 5px solid #eee;
  box-shadow: 0 3px 2px rgba(0, 0, 0, 0.3);  
}

.profile-name {
  padding: 45px;
}

.fa-2x {
    padding-right: 20px;
}

.well-agency {padding:10px;margin-bottom:0;margin:10px 0;}
.well-agency:hover {background:#fff;}
.well-agency a {text-decoration:none;}
.well-service .lead, .well-agency .lead, .well-civic-official .lead {display:block;margin-bottom:5px;font-size:18px;line-height:22px;}
.well-agency img {border-radius:0px;max-width:150px;float:left;margin:0 10px 10px 0; }

.well-service img {margin-bottom:5px;}
.well-service a {display:block;}

.well-agency .lead {margin-top:10px;}
.well-agency hr {margin-top: 0;margin-bottom: 0px;clear: both;padding-top: 10px;}

.sub-info {
  font-size: 16px;
}