/*Main Navigation Bar*/
.navbar-inverse {
    background-color: #fff;
    -moz-box-shadow: 0 5px 6px rgba(204, 204, 204, 0.5);
    -webkit-box-shadow: 0 5px 6px rgba(204, 204, 204, 0.5);
    box-shadow: 0 5px 6px rgba(204, 204, 204, 0.5);
    height: 80px;
    border-color: #fff;
}

.navbar-brand {
    padding-left: 50px;
}

.navbar-inverse .navbar-nav>.active>a, .navbar-inverse .navbar-nav>.active>a:focus, .navbar-inverse .navbar-nav>.active>a:hover {
    color: #ffffff;
    background-color: #505050;
    padding-top: 30px;
    padding-bottom: 30px;
}

.nav>li>a {
    position: relative;
    display: block;
    padding: 30px 15px;
    font-size: 16px;
    font-family: Lato-Light;
}

.nav>li>a:hover {
    color: #fff;
    background-color: #505050;
}


.navbar-inverse .navbar-nav>li>a:hover {
    color: #ffffff;
    background-color: #505050;
}

/*Second Level Navigation Menu*/
.nav>li>a.second_menu {
    position: relative;
    display: block;
    padding: 10px 70px;
    font-size: 20px;
    font-family: Lato-Light;
    background-color: #D1D2D4;
    color: #505050;
    border-right: 1px solid #FCFCFC;
}

.nav>li>a.second_menu:hover {
    background-color: #505050;
    color: #ffffff;
}


.nav>.active>a.second_menu {
    color: #ffffff;
    background-color: #505050;
    border-right: 0px;
}

.sMenuIcon {
    margin-right: 8px;
    width: 40px;
}


/*Third Level Navigation Menu*/
.nav>li>a.third_menu {
    position: relative;
    display: block;
    padding: 5px 10px;
    font-family: Lato-Light;
    font-size: 18px;
    color: #505050;
    border-right: 1px solid #51A8B1;
}

.nav>li>a.third_menu:hover {
    background-color: #505050;
    color: #ffffff;
}

.nav>.active>a.third_menu {
    color: #ffffff;
    background-color: #505050;
    border-right: 0px;
    border-left: 0px;
}

.thirdMenuIcon {
    margin-right: 8px;
    margin-left: 8px;
    width: 17px;
}

/*BreadCRUMBS  Menu*/
.breadcrumbMenu{
    margin-top: 10px;
    margin-left: 9%;
}

.third_menu_nav, .breadcrumbMenu{
    margin-top: 10px;
    margin-left: 7%;
}

.nav>li>a.breadcrumb {
    position: relative;
    display: block;
    padding: 0px 5px;
    font-family: Lato-LightItalic;
    font-size: 14px;
    color: #51A8B1;
}

.nav>li>a.breadcrumb:after {
    content: ">";
    font-size: 16px;
    color: #397B83;
    margin-left: 5px;
}

.nav>li>a.breadcrumb:hover {
    background: transparent;
    padding: 0px;
}

.nav>.active>a.breadcrumb {
    background-color: transparent;
    padding: 0px;
}


