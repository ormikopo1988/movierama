/*
 * Style tweaks
 * --------------------------------------------------
 */
html,
body {
  overflow-x: hidden; /* Prevent scroll on narrow devices */
}
body {
  padding-top: 70px;
  background-image: url("../img/white_wall_hash.png");
}

footer {
    background: #fff;
    padding: 20px 10px;
    -moz-box-shadow: 0 -5px 6px rgba(204, 204, 204, 0.5);
    -webkit-box-shadow: 0 -5px 6px rgba(204, 204, 204, 0.5);
    box-shadow: 0 -5px 6px rgba(204, 204, 204, 0.5);
}

/*
 * Off Canvas
 * --------------------------------------------------
 */
@media screen and (max-width: 767px) {
  .row-offcanvas {
    position: relative;
    -webkit-transition: all .25s ease-out;
         -o-transition: all .25s ease-out;
            transition: all .25s ease-out;
  }

  .row-offcanvas-right {
    right: 0;
  }

  .row-offcanvas-left {
    left: 0;
  }

  .row-offcanvas-right
  .sidebar-offcanvas {
    right: -50%; /* 6 columns */
  }

  .row-offcanvas-left
  .sidebar-offcanvas {
    left: -50%; /* 6 columns */
  }

  .row-offcanvas-right.active {
    right: 50%; /* 6 columns */
  }

  .row-offcanvas-left.active {
    left: 50%; /* 6 columns */
  }

  .sidebar-offcanvas {
    position: absolute;
    top: 0;
    width: 50%; /* 6 columns */
  }
}

.sidebar-outer {
    position: relative;
}

@media (min-width: 768px) {
    .sidebar {
        position: fixed;
        z-index: 9999;
    }
}

@media (max-width: 381px) {
    .sidebar {

    }
}

